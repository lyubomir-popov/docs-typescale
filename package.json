{
  "name": "docs-typescale",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"node scripts/watch.js\" \"live-server --port=3000 --open=/dist/demos/typography-default.html --entry-file=dist/demos/typography-default.html --watch=dist\"",
    "watch": "node scripts/watch.js",
    "build": "node scripts/watch.js --build-only",
    "generate": "npx @lyubomir-popov/baseline-nudge-generator generate config/typography-config.json",
    "generate:default": "npx @lyubomir-popov/baseline-nudge-generator generate config/typography-config.json",
    "generate:editorial": "npx @lyubomir-popov/baseline-nudge-generator generate config/typography-config-editorial.json",
    "build:default": "sass --load-path=node_modules src/default/main.scss:dist/css/default.css --style=compressed",
    "build:editorial": "sass --load-path=node_modules src/editorial/main.scss:dist/css/editorial.css --style=compressed",
    "demo:default": "open dist/demos/typography-default.html",
    "demo:editorial": "open dist/demos/typography-editorial.html"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@lyubomir-popov/baseline-nudge-generator": "^1.3.1",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.0",
    "live-server": "^1.2.2",
    "sass": "^1.89.2",
    "vanilla-framework": "^4.26.0"
  }
}
